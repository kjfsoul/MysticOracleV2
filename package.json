{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "fix-deps": "bash scripts/fix-dependencies.sh",
    "dev": "tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:netlify": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "postinstall": "npm install vite --save-dev",
    "agents:start": "node scripts/start-agents.js",
    "agents:stop": "node scripts/stop-agents.js",
    "agents:status": "node scripts/status-agents.js",
    "agents:progress": "node scripts/progress-agents.js",
    "agents:logs": "cat .mcp/logs/agent-*.log",
    "agents:add-task": "node scripts/agents.js add-task",
    "agents:run": "node scripts/run-agent-system.js",
    "mcp-agents:status": "node scripts/status-mcp-agents.js",
    "mcp-agents:logs": "cat cline_docs/agent-logs/mcp-agent-*.log",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "migrate-ai-system": "NODE_OPTIONS='--loader ts-node/esm' node scripts/migrate-ai-system.ts",
    "start:ai-core": "ts-node ai-core/index.ts",
    "test:ai-system": "jest ai-core/__tests__",
    "consolidate:ai": "ts-node scripts/consolidate-ai-system.ts",
    "start:agents": "ts-node ai-core/scripts/start.ts",
    "status:agents": "ts-node ai-core/scripts/status.ts",
    "test:agents": "jest ai-core/__tests__"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "axios": "^1.8.4",
    "crewai": "^1.0.1",
    "embedchain": "^0.0.8",
    "esbuild": "^0.25.2",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "langchain": "^0.3.21",
    "openai": "^4.93.0",
    "pdfjs-dist": "^5.1.91"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4",
    "vite": "^6.2.6"
  }
}
