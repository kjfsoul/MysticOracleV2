{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "fix-deps": "bash scripts/fix-dependencies.sh",
    "dev": "tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:netlify": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "postinstall": "npm install vite --save-dev",
    "agents:start": "node scripts/start-agents.js",
    "agents:stop": "node scripts/stop-agents.js",
    "agents:status": "node scripts/status-agents.js",
    "agents:progress": "node scripts/progress-agents.js",
    "agents:logs": "cat .mcp/logs/agent-*.log",
    "agents:add-task": "node scripts/agents.js add-task",
    "agents:run": "node scripts/run-agent-system.js",
    "mcp-agents:status": "node scripts/status-mcp-agents.js",
    "mcp-agents:logs": "cat cline_docs/agent-logs/mcp-agent-*.log",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "test": "vitest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "migrate-ai-system": "NODE_OPTIONS='--loader ts-node/esm' node scripts/migrate-ai-system.ts",
    "start:ai-core": "ts-node ai-core/index.ts",
    "test:ai-system": "jest ai-core/__tests__",
    "consolidate:ai": "ts-node scripts/consolidate-ai-system.ts",
    "start:agents": "node scripts/start-agents.js",
    "status:agents": "ts-node ai-core/scripts/status.ts",
    "test:agents": "jest ai-core/__tests__",
    "mcp:install": "echo 'MCP server installation skipped'",
    "mcp:start": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' ./node_modules/.bin/mcp-server-everything --port 3001 --log-level debug --config .mcp/config.yaml",
    "mcp:logs": "tail -f .mcp/logs/server.log",
    "deploy": "netlify deploy --prod",
    "agents:configure": "node node_modules/autonomous-agent-system/src/configure-agent-env.js",
    "agents:away:enable": "node node_modules/autonomous-agent-system/scripts/enable-away-mode.js",
    "agents:away:disable": "node node_modules/autonomous-agent-system/scripts/disable-away-mode.js",
    "agent:sme": "node scripts/interactive-sme-agent.js",
    "agent:web-crawler": "node scripts/web-crawler-agent.js",
    "verify:tarot": "node scripts/verify-tarot-images.js"
  },
  "dependencies": {
    "@hookform/resolvers": "5.0.1",
    "@radix-ui/react-avatar": "1.1.4",
    "@radix-ui/react-dialog": "1.1.7",
    "@radix-ui/react-dropdown-menu": "2.1.7",
    "@radix-ui/react-label": "2.1.3",
    "@radix-ui/react-popover": "1.1.7",
    "@radix-ui/react-progress": "1.1.3",
    "@radix-ui/react-select": "2.1.7",
    "@radix-ui/react-separator": "1.1.3",
    "@radix-ui/react-slot": "1.2.0",
    "@radix-ui/react-switch": "1.1.4",
    "@radix-ui/react-tabs": "1.1.4",
    "@radix-ui/react-toast": "1.2.7",
    "@radix-ui/react-tooltip": "1.2.0",
    "@supabase/supabase-js": "2.49.4",
    "@tailwindcss/postcss": "4.1.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "axios": "^1.8.4",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "crewai": "^1.0.1",
    "date-fns": "4.1.0",
    "embedchain": "^0.0.8",
    "esbuild": "^0.25.2",
    "express-rate-limit": "7.5.0",
    "express-session": "1.18.1",
    "framer-motion": "12.7.3",
    "fs-extra": "11.3.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "langchain": "^0.3.21",
    "lucide-react": "0.488.0",
    "memorystore": "1.6.7",
    "node-geocoder": "4.4.1",
    "openai": "^4.93.0",
    "passport": "0.7.0",
    "passport-local": "1.0.0",
    "pdfjs-dist": "^5.1.91",
    "react-day-picker": "9.6.7",
    "react-helmet": "^6.1.0",
    "react-hook-form": "7.55.0",
    "sharp": "0.34.1",
    "sweph": "2.10.3-4",
    "tailwind-merge": "3.2.0",
    "tw-animate-css": "1.2.5",
    "wouter": "3.7.0",
    "zod": "3.24.2",
    "zod-validation-error": "3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/container-queries": "0.1.1",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/typography": "0.5.16",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/fs-extra": "11.0.4",
    "@types/jest": "29.5.14",
    "@types/node": "22.14.1",
    "@types/testing-library__jest-dom": "5.14.9",
    "@vitejs/plugin-react": "4.4.0",
    "autoprefixer": "10.4.21",
    "dotenv": "16.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "portfinder": "1.0.36",
    "postcss": "8.5.3",
    "tailwindcss": "4.1.4",
    "ts-jest": "^29.0.3",
    "ts-node": "10.9.2",
    "typescript": "4.9.5",
    "vite": "6.3.0"
  }
}
